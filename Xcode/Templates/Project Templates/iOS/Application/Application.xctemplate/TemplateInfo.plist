<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.Xcode3.ProjectTemplateUnitKind</string>

	<key>Identifier</key>
    <string>me.funbox.xcode.templates.iphoneos.applicationBase</string>

	<key>Ancestors</key>
	<array>
		<string>com.apple.dt.unit.cocoaTouchApplicationBase</string>
        <string>me.funbox.xcode.licenses.APL</string>
        <!-- Lagacy Prefixable Supports -->
        <string>com.apple.dt.unit.prefixable</string>
	</array>

    <key>Concrete</key>
    <true />

    <key>Description</key>
    <string>Provides a starting point for any CocoaTouch-based application, along with Bridging header supports for Swift.</string>

    <key>Platforms</key>
    <array>
        <string>com.apple.platform.iphoneos</string>
    </array>

	<key>Definitions</key>
	<dict>
        <!-- imports -->
        <key>*:import:UIKit</key>
        <string>#import &lt;UIKit/UIKit.h&gt;</string>
        <key>*:import:Foundation</key>
        <string>#import &lt;Foundation/Foundation.h&gt;</string>
	</dict>

	<key>Nodes</key>
	<array>
	</array>

	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>languageChoice</string>
			<key>Units</key>
			<dict>
				<key>Objective-C</key>
				<dict>
					<key>Definitions</key>
					<dict>
                        <key>AppDelegate.h</key>
                        <dict>
                            <key>Group</key>
                            <string>Sources</string>
                            <key>Path</key>
                            <string>Sources/AppDelegate.h</string>
                        </dict>
                        <key>AppDelegate.m</key>
                        <dict>
                            <key>Group</key>
                            <string>Sources</string>
                            <key>Path</key>
                            <string>Sources/AppDelegate.m</string>
                        </dict>
					</dict>

                    <key>Nodes</key>
                    <array>
                        <string>AppDelegate.h</string>
                        <string>AppDelegate.m</string>
                    </array>
				</dict>

				<key>Swift</key>
				<dict>
					<key>Definitions</key>
					<dict>
                        <key>AppDelegate.swift</key>
                        <dict>
                            <key>Group</key>
                            <string>Sources</string>
                            <key>Path</key>
                            <string>Sources/AppDelegate.swift</string>
                        </dict>

                        <!-- Bridging supporting for Swift -->
                        <key>Bridging-Header.h</key>
                        <dict>
                            <key>Group</key>
                            <string>Supporting Files</string>
                        </dict>
					</dict>


                    <key>Nodes</key>
                    <array>
                        <string>AppDelegate.swift</string>
                        <string>Bridging-Header.h</string>
                        <string>Bridging-Header.h:APLV2</string>
                        <string>Bridging-Header.h:import:UIKit</string>
                        <string>Bridging-Header.h:import:Foundation</string>
                    </array>

                    <!-- Bridging Header Building Settings for Swift -->
                    <key>Project</key>
                    <dict>
                        <key>SDK</key>
                        <string>iphoneos</string>
                        <key>Configurations</key>
                        <dict>
                            <key>Debug</key>
                            <dict>
                                <key>SWIFT_OBJC_BRIDGING_HEADER</key>
                                <string>$(PROJECT_NAME)/Bridging-Header.h</string>
                            </dict>
                            <key>Release</key>
                            <dict>
                                <key>SWIFT_OBJC_BRIDGING_HEADER</key>
                                <string>$(PROJECT_NAME)/Bridging-Header.h</string>
                            </dict>
                        </dict>
                    </dict>

				</dict>
			</dict>
		</dict>
	</array>

</dict>
</plist>
