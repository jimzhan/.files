ro/SublimeYammy/master/packages.json": [{"labels": [], "previous_names": [], "author": "Vlad Glushchuk", "donate": null, "releases": [{"date": "2013-07-15 13:52:38", "sublime_text": "<3000", "url": "https://github.com/leporo/SublimeYammy/zipball/v1_1", "platforms": ["*"], "version": "1.1"}], "buy": null, "issues": null, "name": "Yammy Syntax Highlighting", "readme": null, "homepage": "https://github.com/leporo/SublimeYammy", "description": "Highlighting definitions for Yammy (https://bitbucket.org/quasinerd/yammy) template syntax."}], "https://raw.githubusercontent.com/naomichi-y/string_counter/master/packages.json": [{"labels": [], "previous_names": [], "author": "Naomichi Yamakita", "donate": null, "releases": [{"date": "2012-09-17 00:00:00", "sublime_text": "<3000", "url": "https://codeload.github.com/naomichi-y/string_counter/zipball/v1.0.0", "platforms": ["*"], "version": "1.0.0"}], "buy": null, "issues": null, "name": "String Counter", "readme": null, "homepage": "https://github.com/naomichi-y/string_counter", "description": "Count number of characters"}], "https://raw.githubusercontent.com/shivkumarganesh/VisSub/master/packages.json": [{"labels": [], "previous_names": [], "author": "Shiv Kumar Ganesh (@shivkumarganesh)", "donate": null, "releases": [{"date": "2011-10-13 05:45:00", "sublime_text": "<3000", "url": "https://codeload.github.com/shivkumarganesh/VisSub/zipball/v1.0", "platforms": ["*"], "version": "1.0"}], "buy": null, "issues": null, "name": "VisSub", "readme": null, "homepage": "https://github.com/shivkumarganesh/VisSub", "description": "A Sublime Plugin for Visage/VisageFX application with gradle build support"}], "https://raw.githubusercontent.com/AutoIt/SublimeAutoItScript/master/packages.json": [{"labels": [], "previous_names": [], "author": "AutoIt", "donate": "https://www.gittip.com/on/github/AutoIt/", "releases": [{"date": "2014-03-15 05:18:06", "sublime_text": "*", "url": "https://codeload.github.com/AutoIt/SublimeAutoItScript/zip/1.3.6",