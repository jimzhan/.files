# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true


[**.{css,html,js,json,jsx,xml,vim,scss,ts}]
indent_size = 2
indent_style = space


[**.{go,go}]
indent_size = 4
indent_style = space
